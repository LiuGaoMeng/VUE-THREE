{
  "name": "aui",
  "version": "0.1.4",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib ./src/aui/index.js --dest dist --name aui",
    "lint": "vue-cli-service lint",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs"
  },
  "dependencies": {
    "@arcgis/core": "^4.20.2",
    "cesium": "1.71.0",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.0",
    "highlight.js": "^10.6.0",
    "three": "^0.132.2",
    "view-design": "^4.5.0",
    "vue": "^2.6.11",
    "vue-highlightjs": "^1.3.3",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "vue-cli-plugin-cesium": "^1.1.7",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.8.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/valid-template-root": "off",
      "no-unused-vars": "off",
      "no-dupe-keys":"off",
      "no-debugger": "off", 
      "no-console":"off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "main": "dist/aui.umd.min.js"
}
